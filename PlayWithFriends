#include<iostream>
using namespace std;
int main()
{
    int t=0,a1,b;
    cin>>t;
    int a[]={6,2,5,5,4,5,6,3,7,6};
    for(int i=0;i<t;i++)
    {
       cin>>a1>>b;
       int sum=a1+b;
       int s=0;
       while(sum!=0)
       {
            int d=sum%10;
            s+=a[d];
            sum=sum/10;
       }
       cout<<s<<endl;
    }
    return 0;
}
